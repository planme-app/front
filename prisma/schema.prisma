datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user_tb {
  user_id    String   @id @default(uuid()) @db.VarChar(36)
  passwd     String
  email      String   @unique
  name       String
  created_at DateTime @default(now())
}
